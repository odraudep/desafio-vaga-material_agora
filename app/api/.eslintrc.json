{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-plusplus": "off",
        "consistent-return": "off",
        "no-await-in-loop": "off"
    }
}
